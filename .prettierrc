{
    "$schema": "http://json.schemastore.org/prettierrc",
    "semi": true,
    "tabWidth": 4,
    "trailingComma": "none",
    "endOfLine": "auto",
    "printWidth": 120,
    "overrides": [
        {
            "files": "*.yml",
            "options": {
                "tabWidth": 2,
                "useTabs": false
            }
        }
    ]
}
